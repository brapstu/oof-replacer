<Window x:Class="Oof_replacer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Oof_replacer"
        
        mc:Ignorable="d"
        Title="Oof Replacer" Height="134" Width="394" Icon="/noob head.png" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="replace" Content="Replace!" HorizontalAlignment="Left" Height="47" Margin="16,20,0,0" VerticalAlignment="Top" Width="118" Click="replace_click"/>
        <TextBlock x:Name="Progress" TextWrapping="Wrap" Margin="169,12,10,0" Text="Click on the button to replace ouch.ogg with an older version." VerticalAlignment="Top" TextAlignment="Center" FontSize="16"/>
    </Grid>
</Window>
